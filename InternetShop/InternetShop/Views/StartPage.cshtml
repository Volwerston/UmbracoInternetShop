@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.StartPage>
    @using ContentModels = Umbraco.Web.PublishedContentModels;
    @{
    Layout = "Layout12Cols.cshtml";
    UmbracoHelper umbracoHelper = new UmbracoHelper(UmbracoContext.Current);
    IPublishedContent content = umbracoHelper.TypedContentSingleAtXPath("//startPage");
    var type = content.GetPropertyValue<string>("mainPageCarouselsType");
    }

    @{
    var elements = Model.Content.Children().Where(x => x.IsVisible());
    foreach(var item in elements)
    {
    var el = item.GetPropertyValue<IEnumerable<IPublishedContent>>("myCarousel");
    Html.RenderPartial(type, el);
}

}
